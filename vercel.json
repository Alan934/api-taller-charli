{

  "version": 2,

      "builds": [

        {

            "src": "src/main.ts",

            "use": "@vercel/node"

        }

    ],

  "buildCommand": "npm run build",

  "installCommand": "npm install && npx prisma generate",

  "routes": [

      {

          "src": "/api/(.*)",

          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],

          "dest": "src/main.ts"

      }

  ],

  "env": {

    "NODE_ENV": "production"

  }

}
