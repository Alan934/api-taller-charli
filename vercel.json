{

  "version": 2,

      "builds": [

        {

            "src": "api/index.ts",

            "use": "@vercel/node"

        }

    ],

  "functions": {

    "api/index.ts": {

      "includeFiles": "prisma/schema.prisma"

    }

  },

  "routes": [

      {

          "src": "/(.*)",

          "dest": "api/index.ts"

      }

  ],

  "env": {

    "NODE_ENV": "production"

  }

}
